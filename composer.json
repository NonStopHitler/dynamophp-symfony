{
  "name": "edumarques/dynamophp-symfony",
  "description": "Symfony bundle that wraps DynamoPHP and simplifies development on Symfony applications.",
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo Marques",
      "email": "eduardomarqs1@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "edumarques/dynamophp": "^0.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.12",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.1",
    "slevomat/coding-standard": "^8.18",
    "symfony/var-dumper": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "EduardoMarques\\DynamoPHPBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EduardoMarques\\DynamoPHPBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
